msgstr "AppleTalk"

#: data/ui/server-hint.ui:94
msgid "File Transfer Protocol"
msgstr "Datņu pārraides protokols (FTP)"

#: data/ui/server-hint.ui:105
msgid "Network File System"
msgstr "Tīkla datņu sistēma (NFS)"

