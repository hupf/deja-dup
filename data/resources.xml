<gresources>
  <!-- intentionally not using profile, to make it easier to reference from vala -->
  <gresource prefix="/org/gnome/DejaDup">
    <file compressed="true">Browser.ui</file>
    <file compressed="true">BrowserGridItem.ui</file>
    <file compressed="true">ConfigFolderList.ui</file>
    <file compressed="true">ConfigFolderPage.ui</file>
    <file compressed="true">ConfigFolderRow.ui</file>
    <file compressed="true">ConfigLocationCombo.ui</file>
    <file compressed="true">ConfigLocationGrid.ui</file>
    <file compressed="true">ConfigServerEntry.ui</file>
    <file compressed="true">MainHeaderBar.ui</file>
    <file compressed="true">MainWindow.ui</file>
    <file compressed="true">PassphraseDialog.ui</file>
    <file compressed="true">PreferencesWindow.ui</file>
    <file compressed="true">ServerHintPopover.ui</file>
    <file compressed="true">TimeCombo.ui</file>

    <!-- files used for OAuth consent screens -->
    <file compressed="true">access-granted.html</file>

    <!-- templates used for pydrive support -->
    <file>pydrive-credentials.json</file>
    <file>pydrive-settings.yaml</file>
  </gresource>

  <gresource prefix="/org/gnome/DejaDup@profile@">
    <!-- ui files that gtk knows about -->
    <file compressed="true" alias="gtk/help-overlay.ui">help-overlay.ui</file>

    <!-- icons -->
    <file compressed="true" alias="icons/scalable/places/deja-dup-google-drive.svg">icons/deja-dup-google-drive.svg</file>
    <file compressed="true" alias="icons/scalable/places/deja-dup-microsoft-onedrive.svg">icons/deja-dup-microsoft-onedrive.svg</file>
    <!-- add our main app icons as fallbacks of last resort -->
    <file compressed="true" alias="icons/scalable/apps/@icon@.svg">icons/@icon@.svg</file>
    <file compressed="true" alias="icons/scalable/apps/@icon@-symbolic.svg">icons/org.gnome.DejaDup-symbolic.svg</file>
  </gresource>
</gresources>
