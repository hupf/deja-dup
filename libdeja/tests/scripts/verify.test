[Operation]
Type=backup
Success=false
Error=Your backup appears to be corrupted.  You should delete the backup and try again.

[Duplicity]
Runs=status;dry;backup;status-restore;verify;

[Duplicity verify]
Script=echo 'Nope' > @XDG_CACHE_HOME@/deja-dup/metadata/README

[Restic]
Runs=backup;verify;

[Restic verify]
Script=echo 'Nope' > @XDG_CACHE_HOME@/deja-dup/metadata/README
