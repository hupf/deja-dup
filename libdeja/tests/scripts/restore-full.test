# Restore everything at once

[Operation]
Type=restore
RestoreTag=full-tag

[Duplicity]
Runs=status-restore;restore;

[Duplicity restore]
ExtraArgs=--time=full-tag

[Restic]
Runs=restore;

[Restic restore]
Snapshot=full-tag
